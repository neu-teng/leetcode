class Solution:
    def maxSubArray(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        this_sum =0
        max_sum = nums[0]
        for i in range(len(nums)):
            if this_sum <0 :
                this_sum =0
            this_sum += nums[i]
            if this_sum > max_sum:
                max_sum = this_sum
        return max_sum
        
 #超过97.52%
