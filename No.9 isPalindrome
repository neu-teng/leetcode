class Solution:
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        first = x 
        if x == 0:
            return True 
        if x <0:
            return False 
        
        res = 0
        while x >0 :
            res = res *10 + x%10
            x = x //10 
            
        return res == first
